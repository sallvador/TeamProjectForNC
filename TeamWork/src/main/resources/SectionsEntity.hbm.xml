<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.SelectionsEntity" table="Sections" schema="public" catalog="postgres">
        <id name="selectionId">
            <column name="section_id" sql-type="integer"/>
        </id>
        <property name="placeId">
            <column name="place_id" sql-type="integer"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar"/>
        </property>
    </class>
</hibernate-mapping>