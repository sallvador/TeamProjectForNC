<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.EventsEntity" table="Events" schema="public" catalog="postgres">
        <id name="eventId">
            <column name="event_id" sql-type="integer"/>
        </id>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="artistId">
            <column name="artist_id" sql-type="integer"/>
        </property>
        <property name="placeId">
            <column name="place_id" sql-type="integer"/>
        </property>
        <property name="info">
            <column name="info" sql-type="text"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar"/>
        </property>
    </class>
</hibernate-mapping>