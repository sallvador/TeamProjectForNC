<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.GenresEntity" table="Genres" schema="public" catalog="postgres">
        <id name="genreId">
            <column name="genre_id" sql-type="integer"/>
        </id>
        <property name="genreName">
            <column name="genre_name" sql-type="varchar"/>
        </property>
    </class>
</hibernate-mapping>