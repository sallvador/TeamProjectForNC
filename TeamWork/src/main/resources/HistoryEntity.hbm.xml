<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.HistoryEntity" table="History" schema="public" catalog="postgres">
        <id name="historyId">
            <column name="history_id" sql-type="integer"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="integer"/>
        </property>
        <property name="eventId">
            <column name="event_id" sql-type="integer"/>
        </property>
        <property name="sectionId">
            <column name="section_id" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>