<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="base.StylesEntity" table="Styles" schema="public" catalog="postgres">
        <property name="priority">
            <column name="priority" sql-type="numeric" precision="-1"/>
        </property>
        <property name="artistId">
            <column name="artist_id" sql-type="integer"/>
        </property>
        <property name="unitId">
            <column name="unit_id" sql-type="integer"/>
        </property>
        <property name="genreId">
            <column name="genre_id" sql-type="integer"/>
        </property>
    </class>
</hibernate-mapping>